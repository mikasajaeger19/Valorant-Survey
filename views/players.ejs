<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Player Data</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      background-color: #f0f0f0;
      margin: 0;
      padding: 0;
    }

    h1 {
      text-align: center;
      margin: 20px;
      color: #1f497d;
    }

    table {
      width: 80%;
      margin: 0 auto;
      border-collapse: collapse;
    }

    th, td {
      padding: 10px;
      text-align: center;
      border: 1px solid #ccc;
    }

    th {
      background-color: #1f497d;
      color: #fff;
    }

    tr:nth-child(even) {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h1>Player Data</h1>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Age</th>
        <th>Role</th>
        <th>Rank</th>
      </tr>
    </thead>
    <tbody>
      <% players.forEach(function(player) { %>
        <tr>
          <td><%= player.name %></td>
          <td><%= player.email %></td>
          <td><%= player.age %></td>
          <td><%= player.role %></td>
          <td><%= player.rank %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
